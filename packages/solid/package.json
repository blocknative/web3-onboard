    "Svelte",
    "Vue",
    "Next",
    "Nuxt",
    "MetaMask",
    "Coinbase",
    "WalletConnect",
    "Ledger",
    "Trezor",
    "Connect Wallet",
    "Ethereum Hooks",
    "Blocknative",
    "Mempool",
    "pending",
    "confirmed",
    "Injected Wallet",
    "Crypto",
    "Crypto Wallet"
  ],
  "repository": {
    "type": "git",
